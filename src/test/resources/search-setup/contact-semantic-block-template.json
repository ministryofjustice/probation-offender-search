{
  "index_patterns": [
    "contact-semantic-block-*"
  ],
  "template": {
    "mappings": {
      "_routing": {
        "required": false
      },
      "properties": {
        "crn": {
          "type": "keyword"
        },
        "timestamp": {
          "type": "date"
        }
      }
    },
    "settings": {
      "index.default_pipeline": "contact-semantic-block-pipeline",
      "number_of_shards": 1,
      "number_of_replicas": 0
    }
  }
}
